        "composer/installers": "~1.0",
        "derhasi/composer-preserve-paths": "0.1.*",
        "drupal/registry_rebuild": "^7.2.3"
    },
    "conflict": {
        "drupal/core": "8.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
      "preferred-install": "source"
    },
    "extra": {
        "installer-paths": {
          "web/": ["type:drupal-core"],
          "web/sites/all/modules/contrib/{$name}/": ["type:drupal-module"],
          "web/sites/all/modules/contrib/salesforce": ["drupal-git/salesforce"],
          "web/sites/all/modules/contrib/tfa_basic/includes/twilio-php/": ["twilio/twilio-php"],
          "web/sites/all/modules/springboard/": ["jacksonriver/springboard_modules"],
          "web/sites/all/themes/contrib/{$name}/": ["type:drupal-theme"],
          "web/sites/all/themes/springboard_themes/": ["jacksonriver/springboard_themes"],
          "web/sites/all/libraries/{$name}/": ["type:drupal-library"],
          "web/sites/all/libraries/litle/": ["jacksonriver/litle-sdk-for-php"],
          "web/sites/all/libraries/ckeditor/": ["ckeditor/ckeditor"],
          "web/sites/all/libraries/fitvids/": ["davatron5000/FitVids.js"],
          "web/sites/all/libraries/salesforce/": ["developerforce/salesforce"],
          "web/sites/all/libraries/mobiledetect/": ["serbanghita/Mobile-Detect"],
          "web/sites/all/libraries/phpuseragent/": ["donatj/PhpUserAgent"],
          "web/sites/all/drush/{$name}/": ["type:drupal-drush"],
          "web/profiles/{$name}/": ["type:drupal-profile"],
          "web/profiles/sbsetup/": ["jacksonriver/springboard_profile"]
        },
        "preserve-paths": [
          "web/sites/all/modules/contrib",
          "web/sites/all/modules/springboard",
          "web/sites/all/themes/contrib",
          "web/sites/all/themes/springboard",
          "web/sites/all/libraries",
          "web/sites/all/drush",
          "web/sites/default/settings.php",
          "web/sites/default/files"
        ],
        "patches": {
          "drupal/commerce_authnet": {
            "Fix to authnet requirements": "https://drupal.org/files/incorrect_requirements-2063787-1.patch",
            "Commerce authnet patch to update URLs": "https://raw.githubusercontent.com/JacksonRiver/Springboard-Build/7.x-4.x/patches/commerce_authnet_update_to_akami.patch?2"
          },
          "drupal/commerce_paypal": {
            "Commerce paypal SSL fix": "https://www.drupal.org/files/issues/use-tls.patch"
          },
          "drupal/encrypt": {
            "Make encrypt module use the encrypt key": "https://drupal.org/files/encrypt_key_is_never_used.patch",
            "Trim only the decrypted output from encrypt": "https://raw.github.com/JacksonRiver/Springboard-Build/7.x-4.12/patches/encrypt-trim-only-decrypted-output.patch"
          },
          "drupal/rules": {
            "1810656 - Makes some of Rules UI compatible with jQuery 1.7": "https://www.drupal.org/files/rules-jquery_update_compatibility-1810656-37.patch"
          },
          "drupal-git/salesforce": {
            "Add idLookup as an option for UPSERT key": "https://drupal.org/files/salesforce-externalId_and_idLookup-1951728.patch",
            "Fix broken test": "https://drupal.org/files/salesforce_mapping-tests-1951744_1.patch",
            "Include only record types that are available": "https://drupal.org/files/salesforce_mapping-available_record_types.patch",
            "Allow connections to test salesforce accounts": "https://drupal.org/files/salesforce-sandox-account-access-1934790-9.patch",
            "Additional caching login in describObject method": "https://drupal.org/files/salesforce-object-additional-caching.patch",
            "Increase the sobject cache from 5 minutes to 1 hour": "https://raw.github.com/JacksonRiver/Springboard-Build/7.x-4.x-sobject_cache/patches/salesforce_increase_sobject_cache_timeout.patch"
          },
          "drupal/strip_utf8mb4": {
            "Node/title/comment stripping": "https://www.drupal.org/files/issues/node_title_comment-2441819-8.patch"
          },
          "drupal/token": {
            "Add a [current-page:object:?] dynamic token (see: https://www.drupal.org/node/919760)": "https://drupal.org/files/token-current_page_object_token-919760-9.patch"
          },
          "drupal/fboauth": {
            "Fix javascript redirect bug with fboauth": "https://www.drupal.org/files/issues/decode_url_before_redirecting_js-2793219-1.patch"
          },
          "drupal/views_litepager": {
            "Remove hook_requirements() to prevent error 'Call to undefined function views_api_version()'": "https://www.drupal.org/files/issues/views_litepager-site-install-requirements-check-error-1874586-10-D7.patch"
          }
        }
    }
}
