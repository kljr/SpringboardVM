{
    "name": "jacksonriver/druvmoser",
    "description": "Springboard with Composer + Drush Make + DrupalVM + Codeception.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jacksonriver/springboard-build",
                "version": "4.14",
                "source": {
                    "url": "https://github.com/JacksonRiver/springboard-build.git",
                    "type": "git",
                    "reference": "7.x-4.x"
                },
                "dist": {
                    "url": "https://github.com/JacksonRiver/springboard-build/archive/7.x-4.14.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jacksonriver/springboard-modules",
                "version": "4.14",
                "source": {
                    "url": "https://github.com/JacksonRiver/springboard_modules.git",
                    "type": "git",
                    "reference": "7.x-4.x"
                },
                "dist": {
                    "url": "https://github.com/JacksonRiver/springboard_modules/archive/7.x-4.14.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jacksonriver/springboard-themes",
                "version": "4.14",
                "source": {
                    "url": "https://github.com/JacksonRiver/springboard-themes.git",
                    "type": "git",
                    "reference": "7.x-4.x"
                },
                "dist": {
                    "url": "https://github.com/JacksonRiver/springboard-themes/archive/7.x-4.14.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jacksonriver/springboard-sdk",
                "version": "4.14",
                "source": {
                    "url": "https://github.com/JacksonRiver/springboard-sdk-php.git",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/JacksonRiver/springboard-sdk-php/archive/7.x-4.12.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jacksonriver/springboard-composer",
                "version": "4.14",
                "source": {
                    "url": "https://github.com/JacksonRiver/springboard-composer-vendor",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/JacksonRiver/springboard-composer-vendor/archive/7.x-4.12.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jacksonriver/acceptance-tests",
                "version": "4.14",
                "source": {
                    "url": "https://github.com/JacksonRiver/acceptance-tests.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.0.20",
        "mnsami/composer-custom-directory-installer": "1.1.*",
        "jacksonriver/springboard-build": "4.14",
        "jacksonriver/acceptance-tests": "4.14",
        "jacksonriver/springboard-themes": "4.14",
        "jacksonriver/springboard-modules": "4.14",
        "jacksonriver/springboard-sdk": "4.14",
        "jacksonriver/springboard-composer": "4.14"
    },
    "require-dev": {
        "geerlingguy/drupal-vm": "^4.2",
        "composer/installers": "~1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
    },
    "config": {
      "preferred-install": "source"
    },
    "extra": {
        "installer-paths": {
            "build": ["jacksonriver/springboard-build"],
            "acceptance-tests": ["jacksonriver/acceptance-tests"],
            "linked/springboard_modules": ["jacksonriver/springboard-modules"],
            "linked/springboard_themes": ["jacksonriver/springboard-themes"],
            "linked/springboard_advocacy": ["jacksonriver/springboard-sdk"],
            "linked/springboard_composer": ["jacksonriver/springboard-composer"]
        }
    },
    "scripts": {
        "post-update-cmd": [
        ]
    }
}
